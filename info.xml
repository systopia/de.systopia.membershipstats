<?xml version="1.0"?>
<extension key="de.systopia.membershipstats" type="module">
  <file>membershipstats</file>
  <name>Membership Stats</name>
  <description>Gathers statistics on contact memberships</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>B. Endres (SYSTOPIA)</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.membershipstats</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.membershipstats</url>
    <url desc="Support">https://github.com/systopia/de.systopia.membershipstats/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-11-07</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>The initial version of this extension was funded by Sculpture Network (www.sculpture-network.org)</comments>
  <civix>
    <namespace>CRM/Membershipstats</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
